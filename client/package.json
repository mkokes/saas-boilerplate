{
  "name": "client",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development NODE_PATH=src react-app-rewired start",
    "build": "cross-env NODE_ENV=production NODE_PATH=src react-app-rewired build",
    "test:clean": "rimraf ./coverage",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test NODE_PATH=src react-app-rewired test --coverage",
    "test:watch": "cross-env NODE_ENV=test NODE_PATH=src react-app-rewired test --watchAll",
    "eject": "react-scripts eject",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "npm run lint:eslint -- .",
    "lint:css": "stylelint './src/**/*.js'",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --fix",
    "generate": "plop --plopfile internals/generators/index.js"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.{json,md}": [
      "prettier --config ../.prettierrc --write",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 10,
        "functions": 1,
        "lines": 1,
        "statements": 1
      }
    },
    "setupFiles": [
      "<rootDir>/internals/testing/enzyme-setup.js"
    ],
    "testMatch": [
      "tests/.*\\.test\\.js$"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "fontfaceobserver": "^2.0.13",
    "loadable-components": "^2.2.3",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.0",
    "sanitize.css": "^8.0.0",
    "styled-components": "^4.0.3",
    "apollo-cache-inmemory": "^1.2.8",
    "apollo-client": "^2.4.0",
    "apollo-link": "^1.2.3",
    "apollo-link-http": "^1.5.5",
    "apollo-link-state": "^0.4.1",
    "apollo-utilities": "^1.0.21",
    "graphql": "^14.0.2",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tag": "^2.9.2",
    "react-apollo": "^2.2.4",
    "eventemitter3": "^3.1.0",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "babel-eslint": "9",
    "babel-plugin-styled-components": "^1.8.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "husky": "^1.1.2",
    "jest-styled-components": "^6.2.2",
    "lint-staged": "^8.0.3",
    "plop": "^2.1.0",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-app-rewired": "^1.6.2",
    "stylelint": "9.3.0",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.3.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
