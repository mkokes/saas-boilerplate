{
  "name": "blog",
  "private": true,
  "version": "1.0.0",
  "workspaces": {
    "nohoist": ["gatsby"]
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop --verbose",
    "format": "prettier --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:ci": "cypress run --reporter junit --reporter-options \"mochaFile=results/e2e-test-output.xml\"",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run:ci"
  },
  "dependencies": {
    "@svgr/webpack": "^4.1.0",
    "babel-eslint": "^10.0.1",
    "date-fns": "^1.30.1",
    "eslint": "^5.11.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.1",
    "gatsby": "^2.0.82",
    "gatsby-codemods": "^1.0.8",
    "gatsby-link": "^2.0.1",
    "gatsby-plugin-feed": "^2.0.11",
    "gatsby-plugin-google-fonts": "^0.0.4",
    "gatsby-plugin-react-helmet": "^3.0.5",
    "gatsby-plugin-sass": "^2.0.7",
    "gatsby-remark-copy-linked-files": "^2.0.8",
    "gatsby-remark-static-images": "^1.0.0",
    "gatsby-source-filesystem": "^2.0.12",
    "gatsby-transformer-remark": "^2.1.19",
    "http-proxy-middleware": "^0.19.1",
    "node-sass": "^4.11.0",
    "normalize.css": "^8.0.1",
    "qs": "^6.6.0",
    "react": "^16.7.0",
    "react-countup": "^4.0.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "cypress": "^3.1.4",
    "eslint-plugin-cypress": "^2.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "start-server-and-test": "^1.7.11"
  }
}
