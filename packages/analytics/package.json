{
  "name": "analytics-server",
  "description": "Analytics server which enables us to work around in-browser ad-blockers",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "deploy": "yarn now -f --local-config=.deploy/live.json && yarn now --local-config=.deploy/live.json alias",
    "release": "yarn deploy && yarn now rm dcabot-analytics --safe --yes"

  },
  "dependencies": {
    "@koa/cors": "^2.2.2",
    "global-tunnel-ng": "^2.6.0",
    "got": "^9.2.2",
    "koa": "^2.5.3",
    "koa-router": "^7.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^4.0.0",
    "now": "^12.1.1"
  }
}
