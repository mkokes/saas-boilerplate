{
  "name": "app",
  "description": "Front-end Application",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "start": "REACT_APP_MODE=dev react-scripts start",
    "prebuild": "npm run build:clean",
    "build:stage": "REACT_APP_MODE=stage react-scripts build",
    "build:live": "REACT_APP_MODE=live react-scripts build",
    "build:clean": "rimraf ./build",
    "test": "REACT_APP_MODE=test react-scripts test --watchAll=false",
    "test:watch": "REACT_APP_MODE=test react-scripts test --watchAll",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "npm run lint:eslint -- .",
    "lint:css": "stylelint './src/**/*.js'",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --fix",
    "pretty": "prettier --config .prettierrc --write \"src/**/*.js\"",
    "generate": "plop --plopfile internals/generators/index.js",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 10,
        "functions": 1,
        "lines": 1,
        "statements": 1
      }
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/free-regular-svg-icons": "^5.8.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@sentry/browser": "5.1.1",
    "apollo-cache-inmemory": "^1.2.8",
    "apollo-client": "^2.4.0",
    "apollo-link": "^1.2.3",
    "apollo-link-error": "1.1.10",
    "apollo-link-http": "^1.5.5",
    "apollo-link-state": "^0.4.1",
    "apollo-utilities": "^1.0.21",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "cross-env": "^5.2.0",
    "eventemitter3": "^3.1.0",
    "fontfaceobserver": "^2.0.13",
    "formik": "^1.3.1",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "jwt-decode": "^2.2.0",
    "loadable-components": "^2.2.3",
    "lodash": "^4.17.10",
    "moment-timezone": "^0.5.23",
    "qrcode.react": "^0.9.1",
    "query-string": "^6.2.0",
    "react": "next",
    "react-apollo": "^2.2.4",
    "react-coinbase-commerce": "^1.3.1",
    "react-confirm-alert": "^2.0.7",
    "react-dom": "next",
    "react-ga": "^2.5.6",
    "react-helmet": "6.0.0-beta",
    "react-moment": "^0.9.1",
    "react-responsive": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.0",
    "react-select": "^2.2.0",
    "react-switch": "^4.1.0",
    "react-table": "^6.8.6",
    "react-toastify": "^5.0.0",
    "reactstrap": "^8.0.0",
    "reaptcha": "^1.4.0",
    "sanitize.css": "^8.0.0",
    "styled-components": "^4.0.3",
    "use-async-effect": "^2.0.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "babel-plugin-styled-components": "^1.8.0",
    "env-cmd": "^8.0.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "jest-styled-components": "^6.2.2",
    "plop": "^2.1.0",
    "stylelint": "10.0.1",
    "stylelint-config-recommended": "2.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.6.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
