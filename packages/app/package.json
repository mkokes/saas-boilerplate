{
  "name": "app",
  "description": "Front-end Application",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "start": "cross-env NODE_ENV=development NODE_PATH=./src env-cmd ./env/dev.env react-scripts start",
    "prebuild": "npm run build:clean",
    "build:stage": "cross-env NODE_ENV=production NODE_PATH=./src env-cmd ./env/stage.env react-scripts build",
    "build:live": "cross-env NODE_ENV=production NODE_PATH=./src env-cmd ./env/prod.env react-scripts build",
    "build:clean": "rimraf ./build",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test NODE_PATH=./src react-scripts test",
    "test:watch": "cross-env NODE_ENV=test NODE_PATH=./src react-scripts test --watchAll",
    "eject": "react-scripts eject",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "npm run lint:eslint -- .",
    "lint:css": "stylelint './src/**/*.js'",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --fix",
    "generate": "plop --plopfile internals/generators/index.js"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.{json,md}": [
      "prettier --config ../.prettierrc --write",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 10,
        "functions": 1,
        "lines": 1,
        "statements": 1
      }
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "apollo-cache-inmemory": "^1.2.8",
    "apollo-client": "^2.4.0",
    "apollo-link": "^1.2.3",
    "apollo-link-error": "1.1.5",
    "apollo-link-http": "^1.5.5",
    "apollo-link-state": "^0.4.1",
    "apollo-utilities": "^1.0.21",
    "bootstrap": "^4.1.3",
    "cross-env": "^5.2.0",
    "eventemitter3": "^3.1.0",
    "fontfaceobserver": "^2.0.13",
    "formik": "^1.3.1",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "jwt-decode": "^2.2.0",
    "loadable-components": "^2.2.3",
    "lodash": "^4.17.10",
    "moment-timezone": "^0.5.23",
    "qrcode.react": "^0.9.1",
    "query-string": "^6.2.0",
    "react": "next",
    "react-apollo": "^2.2.4",
    "react-dom": "next",
    "react-ga": "^2.5.6",
    "react-helmet": "6.0.0-beta",
    "react-moment": "^0.8.4",
    "react-responsive": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "react-select": "^2.2.0",
    "react-switch": "^3.0.4",
    "react-table": "^6.8.6",
    "react-toastify": "^4.5.0",
    "reactstrap": "^7.0.2",
    "reaptcha": "^1.4.0",
    "sanitize.css": "^8.0.0",
    "styled-components": "^4.0.3",
    "use-async-effect": "^2.0.0",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "babel-eslint": "9",
    "babel-plugin-styled-components": "^1.8.0",
    "env-cmd": "^8.0.2",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.3",
    "husky": "^1.1.2",
    "jest-styled-components": "^6.2.2",
    "lint-staged": "^8.0.3",
    "plop": "^2.1.0",
    "prettier": "1.15.3",
    "stylelint": "9.9.0",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.5.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
